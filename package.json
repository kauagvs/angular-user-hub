{
  "name": "angular-user-hub",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "api": "node middleware.js",
    "populate-db": "node populate-db.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve:ssr:angular-user-hub": "node dist/angular-user-hub/server/server.mjs",
    "start:all": "concurrently \"npm run populate-db\" \"npm run api\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/platform-server": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@angular/ssr": "^18.0.3",
    "@material-tailwind/html": "^2.2.2",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "json-server": "^1.0.0-beta.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "json-server-auth": "^2.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2"
  }
}
